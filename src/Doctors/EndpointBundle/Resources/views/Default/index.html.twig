    <script>
        $("#btn-sub").click(function () {
            //alert("je suis là");
            var bol = true;
            var reg = new RegExp('^[a-z0-9]+([_|\.|-]{1}[a-z0-9]+)*@[a-z0-9]+([_|\.|-]{1}[a-z0-9]+)*[\.]{1}[a-z]{2,6}$', 'i');

            var email = $("#email").val();
            if (email == "")
            {
                $("#email").css({'border-color': '#DB1702'});
                bol = false;
            }
            if (email != "")
            {
                console.log(email);
                if (reg.test(email) == false)
                {
                    $("#email").css({'border-color': '#DB1702'});
                    bol = false;
                } else
                {
                    $("#email").css({'border-color': '#3A9D23'});
                    bol = true;
                }

            }
            if (bol == true) {
                var fromData = {email: $("#email").val()};
                $.post("{{ path('add_emails') }}", fromData, function (data) {
                    $("#email").val("");
                    alert("تم تسجيل البريد بنجاح");
                });
            }


        });
